.lock-cav{
  /* position: absolute;
  top: 240rpx;
  left: 100rpx;
  right: 0;
  z-index: 5; */
  display:block;
  text-align: center;
  width: 300rpx;
  height: 300rpx;
  margin-left: auto;
  margin-right: auto;
 
}
.lock-msg{
  position: absolute;
  top:220rpx;
  width: 100%;
  text-align: center;
  display:block;
  font-size: 28rpx;
  color: #FD5702;
}
.lock-title{
  position: absolute;
  top:80%;
  width: 100%;
  text-align: center;
  display:block;
  font-size: 28rpx;
  color: #262626;
  height: 40rpx;
  line-height: 40rpx ;
font-weight: 500;
}
.lock-title.error{
  color: #e64340
}
.lock-title.succ{
  color: #2E98FF
}
.lock-reset{
  position: absolute;
  top: 80rpx;
  right: 122rpx; 
  color:#10AEFF; 
  display:block;
  font-size: 36rpx;
  height: 40rpx;
  line-height: 40rpx 
}
.lock-reset:active{
  opacity: 0.8
}