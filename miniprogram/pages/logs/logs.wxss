  display: flex;
  flex-direction: column;
  height: 100%;
  width: 100%;
}
.log-item {
  margin: 10rpx;
}
.lock-box{
  /* width:100vw;
  height: 100vh; */
  background-color: #fff;
 
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items:center;
  /* justify-content: space-between; */
 
  /* position: fixed;
